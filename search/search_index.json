u5bcc\u5f3a\u3001\u6c11\u4e3b\u3001\u6587\u660e\u3001\u548c\u8c10\uff1b\u81ea\u7531\u3001\u5e73\u7b49\u3001\u516c\u6b63\u3001\u6cd5\u6cbb\uff1b\u7231\u56fd\u3001\u656c\u4e1a\u3001\u8bda\u4fe1\u3001\u53cb\u5584 \u670d\u52a1\u5668 \u8150\u7af9\u548cOP\u5c3d\u91cf\u4e3a\u73a9\u5bb6\u5e26\u6765\u66f4\u8212\u9002\u7684\u6e38\u620f\u4f53\u9a8c\uff0c\u4e14\u63d0\u4f9b\u5408\u7406\u7684\u670d\u52a1 \u670d\u52a1\u5668\u548c\u7fa4\u5185\u7684\u89c4\u5219\u90fd\u662f\u7531\u8150\u7af9\u6216OP\u51b3\u5b9a\u7684 \u672c\u670d\u52a1\u5668\u4e3a\u516c\u76ca\u670d\u52a1\u5668\uff0c\u9075\u7167Minecraft EULA \u672c\u670d\u52a1\u5668\u63a5\u53d7\u65e0\u507f\u8d5e\u52a9\uff0c\u6240\u6709\u7684\u8d5e\u52a9\u5c06\u4f1a\u7528\u4e8e \u670d\u52a1\u5668\u7684\u5185\u7f51\u6620\u5c04\u8d39\u7528\u3001\u63d2\u4ef6\u8d2d\u4e70\u8d39\u7528\u3001\u57df\u540d\u8d39\u7528 \u670d\u52a1\u5668\u5b81\u7f3a\u6bcb\u6ee5\uff0c\u8ffd\u6c42\u8f83\u9ad8\u8d28\u91cf\u7684\u793e\u533a\u548c\u6e38\u620f\u73af\u5883 \u793e\u533a\u73af\u5883\u4e0e\u4e2a\u4eba \u4e0d\u5f97\u8fb1\u9a82\u3001\u8bfd\u8c24\u3001\u9020\u8c23\u4ed6\u4eba \u4e0d\u5f97\u6b3a\u9a97\u3001\u8bef\u5bfc\u4ed6\u4eba \u4e0d\u5f97\u6076\u610f\u5f00\u4ed6\u4eba\u7684\u73a9\u7b11\uff0c\u6216\u662f\u8bd5\u63a2\u4ed6\u4eba\u7684\u96f7\u533a\u548c\u5e95\u7ebf \u4e0d\u5f97\u8fdd\u53cd\u6cd5\u5f8b\u6cd5\u89c4 \u4e0d\u5f97\u901a\u8fc7\u5404\u79cd\u65b9\u5f0f\u9020\u6210\u4ed6\u4eba\u7684\u635f\u5931\uff0c\u6216\u662f\u7834\u574f\u6e38\u620f\u516c\u5e73\u548c\u4ed6\u4eba\u7684\u6e38\u620f\u4f53\u9a8c \u5305\u62ec\u4ee5\u4e0b\u884c\u4e3a\u88ab\u5224\u5b9a: \u76d7\u7a83\u3001\u5237\u5c4f\u3001\u6076\u610f\u6740\u4eba\u3001\u63a0\u593a\u3001\u7834\u574f\u5efa\u7b51\u3001\u6ee5\u7528\u4ed6\u4eba\u65e0\u507f\u63d0\u4f9b\u7684\u516c\u76ca\u8bbe\u65bd \u4e0d\u5f97\u4ee5\u975e\u6b63\u5e38\u624b\u6bb5\u6e38\u620f \u5305\u62ec\u4ee5\u4e0b\u884c\u4e3a\u88ab\u5224\u5b9a: \u4f7f\u7528\u4f5c\u5f0a\u5ba2\u6237\u7aef\u3001\u81ea\u52a8\u5316\u811a\u672c \u4e0d\u5f97\u79c1\u4e0b\u4ea4\u6613\u6216\u505a\u51fa\u8fdd\u53cdMinecraft EULA\u7684\u884c\u4e3a \u5305\u62ec\u4ee5\u4e0b\u884c\u4e3a\u88ab\u5224\u5b9a: \u4f7f\u7528\u975e\u6cd5\u8d26\u53f7\u3001\u5411\u4ed6\u4eba\u79df\u501f\u53f7 \u4e0d\u63d0\u5021\u4e00\u4eba\u591a\u53f7 \u4e0d\u5f97\u4ee5\u4efb\u4f55\u65b9\u5f0f\u5f3a\u5360\u6216\u4fb5\u5360\u4ed6\u4eba\u571f\u5730 \u82e5\u73a9\u5bb6\u505a\u51fa\u8fdd\u53cd\u9053\u5fb7\u5c42\u9762\u4e0a\u6216\u672a\u63d0\u53ca\u7684\u4e8b\u60c5\uff0cOP\u6839\u636e\u60c5\u51b5\u5904\u7406","title":"\u603b\u89c4"},{"location":"rules/basic-rule/#_1","text":"\u672c\u89c4\u5219\u9002\u7528\u4e8e\u7fa4\u548c\u670d\u52a1\u5668 \u5982\u679c\u4f60\u89c9\u5f97\u670d\u52a1\u5668\u7684\u89c4\u5219\u592a\u591a\uff0c\u5c31\u6309\u7167\u793e\u4f1a\u4e3b\u4e49\u6838\u5fc3\u4ef7\u503c\u89c2\u7684\u7406\u5ff5\u6267\u884c\u5c31\u884c\uff0c\u5dee\u4e0d\u591a \u5bcc\u5f3a\u3001\u6c11\u4e3b\u3001\u6587\u660e\u3001\u548c\u8c10\uff1b\u81ea\u7531\u3001\u5e73\u7b49\u3001\u516c\u6b63\u3001\u6cd5\u6cbb\uff1b\u7231\u56fd\u3001\u656c\u4e1a\u3001\u8bda\u4fe1\u3001\u53cb\u5584","title":"\u603b\u89c4"},{"location":"rules/basic-rule/#_2","text":"\u8150\u7af9\u548cOP\u5c3d\u91cf\u4e3a\u73a9\u5bb6\u5e26\u6765\u66f4\u8212\u9002\u7684\u6e38\u620f\u4f53\u9a8c\uff0c\u4e14\u63d0\u4f9b\u5408\u7406\u7684\u670d\u52a1 \u670d\u52a1\u5668\u548c\u7fa4\u5185\u7684\u89c4\u5219\u90fd\u662f\u7531\u8150\u7af9\u6216OP\u51b3\u5b9a\u7684 \u672c\u670d\u52a1\u5668\u4e3a\u516c\u76ca\u670d\u52a1\u5668\uff0c\u9075\u7167Minecraft EULA \u672c\u670d\u52a1\u5668\u63a5\u53d7\u65e0\u507f\u8d5e\u52a9\uff0c\u6240\u6709\u7684\u8d5e\u52a9\u5c06\u4f1a\u7528\u4e8e \u670d\u52a1\u5668\u7684\u5185\u7f51\u6620\u5c04\u8d39\u7528\u3001\u63d2\u4ef6\u8d2d\u4e70\u8d39\u7528\u3001\u57df\u540d\u8d39\u7528 \u670d\u52a1\u5668\u5b81\u7f3a\u6bcb\u6ee5\uff0c\u8ffd\u6c42\u8f83\u9ad8\u8d28\u91cf\u7684\u793e\u533a\u548c\u6e38\u620f\u73af\u5883","title":"\u670d\u52a1\u5668"},{"location":"rules/basic-rule/#_3","text":"\u4e0d\u5f97\u8fb1\u9a82\u3001\u8bfd\u8c24\u3001\u9020\u8c23\u4ed6\u4eba \u4e0d\u5f97\u6b3a\u9a97\u3001\u8bef\u5bfc\u4ed6\u4eba \u4e0d\u5f97\u6076\u610f\u5f00\u4ed6\u4eba\u7684\u73a9\u7b11\uff0c\u6216\u662f\u8bd5\u63a2\u4ed6\u4eba\u7684\u96f7\u533a\u548c\u5e95\u7ebf \u4e0d\u5f97\u8fdd\u53cd\u6cd5\u5f8b\u6cd5\u89c4 \u4e0d\u5f97\u901a\u8fc7\u5404\u79cd\u65b9\u5f0f\u9020\u6210\u4ed6\u4eba\u7684\u635f\u5931\uff0c\u6216\u662f\u7834\u574f\u6e38\u620f\u516c\u5e73\u548c\u4ed6\u4eba\u7684\u6e38\u620f\u4f53\u9a8c \u5305\u62ec\u4ee5\u4e0b\u884c\u4e3a\u88ab\u5224\u5b9a: \u76d7\u7a83\u3001\u5237\u5c4f\u3001\u6076\u610f\u6740\u4eba\u3001\u63a0\u593a\u3001\u7834\u574f\u5efa\u7b51\u3001\u6ee5\u7528\u4ed6\u4eba\u65e0\u507f\u63d0\u4f9b\u7684\u516c\u76ca\u8bbe\u65bd \u4e0d\u5f97\u4ee5\u975e\u6b63\u5e38\u624b\u6bb5\u6e38\u620f \u5305\u62ec\u4ee5\u4e0b\u884c\u4e3a\u88ab\u5224\u5b9a: \u4f7f\u7528\u4f5c\u5f0a\u5ba2\u6237\u7aef\u3001\u81ea\u52a8\u5316\u811a\u672c \u4e0d\u5f97\u79c1\u4e0b\u4ea4\u6613\u6216\u505a\u51fa\u8fdd\u53cdMinecraft EULA\u7684\u884c\u4e3a \u5305\u62ec\u4ee5\u4e0b\u884c\u4e3a\u88ab\u5224\u5b9a: \u4f7f\u7528\u975e\u6cd5\u8d26\u53f7\u3001\u5411\u4ed6\u4eba\u79df\u501f\u53f7 \u4e0d\u63d0\u5021\u4e00\u4eba\u591a\u53f7 \u4e0d\u5f97\u4ee5\u4efb\u4f55\u65b9\u5f0f\u5f3a\u5360\u6216\u4fb5\u5360\u4ed6\u4eba\u571f\u5730 \u82e5\u73a9\u5bb6\u505a\u51fa\u8fdd\u53cd\u9053\u5fb7\u5c42\u9762\u4e0a\u6216\u672a\u63d0\u53ca\u7684\u4e8b\u60c5\uff0cOP\u6839\u636e\u60c5\u51b5\u5904\u7406","title":"\u793e\u533a\u73af\u5883\u4e0e\u4e2a\u4eba"},{"location":"rules/donate-rule/","text":"\u8d5e\u52a9 \u76ee\u524d\u4e0d\u53ef\u7528 \u672c\u670d\u52a1\u5668\u7684\u6240\u6709\u8d5e\u52a9\u90fd\u5c06\u7528\u4e8e\u63d0\u4f9b\u6e38\u620f\u670d\u52a1 \u670d\u52a1\u5668\u7684\u8d5e\u52a9\u53ef\u9000 \u670d\u52a1\u5668\u7684\u8d5e\u52a9\u8d26\u5355\u660e\u7ec6 \u8d5e\u52a9\u662f\u65e0\u507f\u7684\uff0c\u4e0d\u4f1a\u5e26\u6765\u4efb\u4f55\u5b9e\u8d28\u4e0a\u7684\u597d\u5904","title":"\u8d5e\u52a9"},{"location":"rules/donate-rule/#_1","text":"\u76ee\u524d\u4e0d\u53ef\u7528 \u672c\u670d\u52a1\u5668\u7684\u6240\u6709\u8d5e\u52a9\u90fd\u5c06\u7528\u4e8e\u63d0\u4f9b\u6e38\u620f\u670d\u52a1 \u670d\u52a1\u5668\u7684\u8d5e\u52a9\u53ef\u9000 \u670d\u52a1\u5668\u7684\u8d5e\u52a9\u8d26\u5355\u660e\u7ec6 \u8d5e\u52a9\u662f\u65e0\u507f\u7684\uff0c\u4e0d\u4f1a\u5e26\u6765\u4efb\u4f55\u5b9e\u8d28\u4e0a\u7684\u597d\u5904","title":"\u8d5e\u52a9"},{"location":"rules/mining-rule/","text":"\u6316\u77ff\u89c4\u5219 \u672c\u89c4\u5219\u9002\u7528\u4e8e\u670d\u52a1\u5668 \u672c\u670d\u52a1\u5668\u4e0d\u9650\u5236\u65e0\u9650\u591c\u89c6(Gamma)\uff0c\u4f46\u5c3d\u91cf\u5728\u6316\u77ff\u65f6\u63d2\u706b\u628a \u5efa\u8bae\u4f7f\u7528\u9c7c\u9aa8\u6316\u77ff\u6cd5","title":"\u6316\u77ff\u89c4\u5219"},{"location":"rules/mining-rule/#_1","text":"\u672c\u89c4\u5219\u9002\u7528\u4e8e\u670d\u52a1\u5668 \u672c\u670d\u52a1\u5668\u4e0d\u9650\u5236\u65e0\u9650\u591c\u89c6(Gamma)\uff0c\u4f46\u5c3d\u91cf\u5728\u6316\u77ff\u65f6\u63d2\u706b\u628a \u5efa\u8bae\u4f7f\u7528\u9c7c\u9aa8\u6316\u77ff\u6cd5","title":"\u6316\u77ff\u89c4\u5219"},{"location":"tutorials/minecraft-server-deployment/","text":"","title":"Minecraft\u670d\u52a1\u5668\u7684\u642d\u5efa"},{"location":"tutorials/minecraft-server-properties/","text":"","title":"\u670d\u52a1\u5668\u914d\u7f6e\u6587\u4ef6\u89e3\u6790"},{"location":"tutorials/minecraft-server-selection/","text":"","title":"Minecraft\u670d\u52a1\u5668\u7684\u9009\u62e9"},{"location":"tutorials/paper-plugin-development/","text":"","title":"Paper\u63d2\u4ef6\u5f00\u53d1"},{"location":"update_log/update_log_2022_2/","text":"2022\u5e742\u6708\u66f4\u65b0\u65e5\u5fd7 2022.2.10 \u5b8c\u5584\u4e86\u6587\u6863 \u66f4\u6539\u4e86\u4e00\u5927\u5806\u63d2\u4ef6\u7684\u914d\u7f6e\u6587\u4ef6 2022.2.9 \u4f18\u5316\u4e86\u5982\u4e0b\u670d\u52a1\u5668\u57fa\u672c\u8bbe\u7f6e: \u4f5c\u8005\uff1a\u6781\u5149creeper \u6d89\u53ca\u6587\u4ef6: bukkit.yml spigot.yml paper.yml \u52a8\u7269\u7684\u533a\u5757\u751f\u5b